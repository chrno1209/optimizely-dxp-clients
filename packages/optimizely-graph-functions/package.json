{
  "name": "@remkoj/optimizely-graph-functions",
  "license": "Apache-2.0",
  "version": "4.1.0",
  "repository": "https://github.com/remkoj/optimizely-dxp-clients.git",
  "author": "Remko Jantzen <693172+remkoj@users.noreply.github.com>",
  "homepage": "https://github.com/remkoj/optimizely-dxp-clients",
  "files": [
    "./dist"
  ],
  "bin": {
    "patch-codegen": "./dist/patch.js"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./plugin": "./dist/index.js",
    "./preset": "./dist/preset.js",
    "./transform": "./dist/transform.js",
    "./documents": "./dist/documents/index.js",
    "./loader": "./dist/embedded-loader.js"
  },
  "typesVersions": {
    "*": {
      "plugin": [
        "dist/index.d.ts"
      ],
      "preset": [
        "dist/preset.d.ts"
      ],
      "documents": [
        "dist/documents/index.d.ts"
      ],
      "transform": [
        "dist/transform.d.ts"
      ],
      "loader": [
        "./dist/embedded-loader.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.5.1",
    "@graphql-codegen/plugin-helpers": "^5.1.0",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "tslib": "^2.8.1"
  },
  "scripts": {
    "clean": "tsc --build --clean",
    "prepare": "tsc --build",
    "recompile": "tsc --build --clean && tsc --build --force",
    "watch": "yarn tsc --watch",
    "test-patch": "node ./dist/patch.js"
  },
  "peerDependencies": {
    "@graphql-typed-document-node/core": "*",
    "@remkoj/optimizely-graph-client": "4.1.0",
    "graphql": "*",
    "graphql-request": "^6.1.0",
    "graphql-tag": "*"
  }
}
